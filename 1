#include <iostream>

void drawRectangle(int N, int K) {
    for (int i = 0; i < N; ++i) {
        for (int j = 0; j < K; ++j) {
            std::cout << "*";
        }
        std::cout << std::endl;
    }
}

int main() {
    int height, width;
    std::cout << "Введите высоту прямоугольника (N): ";
    std::cin >> height;
    std::cout << "Введите ширину прямоугольника (K): ";
    std::cin >> width;

    drawRectangle(height, width);

    return 0;
}
