#include <iostream>
#include <vector>

using namespace std;

bool isPerfect(int num) {
    int sum = 0;
    for (int i = 1; i <= num / 2; ++i) {
        if (num % i == 0) {
            sum += i;
        }
    }
    return sum == num;
}

vector<int> findPerfectNumbers(int start, int end) {
    vector<int> perfectNumbers;
    for (int num = start; num <= end; ++num) {
        if (isPerfect(num)) {
            perfectNumbers.push_back(num);
        }
    }
    return perfectNumbers;
}

int main() {
    int start, end;
    cout << "Enter start of interval: ";
    cin >> start;
    cout << "Enter end of interval: ";
    cin >> end;

    vector<int> perfectNumbers = findPerfectNumbers(start, end);

    cout << "Perfect numbers in the interval [" << start << ", " << end << "]: ";
    for (int num : perfectNumbers) {
        cout << num << " ";
    }
    cout << endl;

    return 0;
}
